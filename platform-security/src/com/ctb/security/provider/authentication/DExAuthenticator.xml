<?xml version="1.0" ?>
<!DOCTYPE MBeanType SYSTEM "commo.dtd">

<MBeanType
 Name          = "DExAuthenticator"
 DisplayName   = "DExAuthenticator"
 Package       = "com.ctb.security.provider.authentication"
 Extends       = "weblogic.management.security.authentication.Authenticator"
 PersistPolicy = "OnUpdate"
>

 <MBeanAttribute
  Name          = "ProviderClassName"
  Type          = "java.lang.String"
  Writeable     = "false"
  Default       = "&quot;com.ctb.security.provider.authentication.DExAuthenticationProviderImpl&quot;"
 />

 <MBeanAttribute
  Name          = "Description"
  Type          = "java.lang.String"
  Writeable     = "false"
  Default       = "&quot;DEx Security Provider Authentication Provider&quot;"
 />

 <MBeanAttribute
  Name          = "Version"
  Type          = "java.lang.String"
  Writeable     = "false"
  Default       = "&quot;1.0&quot;"
 />

 <MBeanAttribute
  Name          = "JDBCDriver"
  Type          = "java.lang.String"
  Writeable     = "true"
  Default       = "&quot;oracle.jdbc.driver.OracleDriver&quot;"
 />


 <MBeanAttribute
  Name          = "DatabaseURL"
  Type          = "java.lang.String"
  Writeable     = "true"
  Default       = "&quot;jdbc:oracle:thin:@168.116.31.32:1521:toast&quot;"
 />

 <MBeanAttribute
  Name          = "User"
  Type          = "java.lang.String"
  Writeable     = "true"
  Default       = "&quot;oas&quot;"
 />

 <MBeanAttribute
  Name          = "Password"
  Type          = "java.lang.String"
  Writeable     = "true"
  Default       = "&quot;oas&quot;"
 />

</MBeanType>
